{
  "name": "milanhirapra/wp-manage-users",
  "description": "View the list of users in HTML table by visiting the custom endpoints.",
  "keywords": [
    "wordpress",
    "users",
    "plugin",
    "api"
  ],
  "license": "GPL-2.0-or-later",
  "type": "wordpress-plugin",
  "homepage": "https://github.com/milanhirapra/wp-manage-users/",
  "authors": [
    {
      "name": "Milan Hirapra",
      "email": "milan.hirpara1e@gmail.com",
      "role": "WordPress Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/milanhirapra/wp-manage-users/issues"
  },
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*",
    "wp-coding-standards/wpcs": "^2",
    "phpcompatibility/phpcompatibility-wp": "^2",
    "dg/composer-cleaner": "^2",
    "phpcompatibility/php-compatibility": "^9.1"
  },
  "autoload": {
    "psr-4": {
      "Milan\\ManageUsers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Milan\\ManageUsers\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "dg/composer-cleaner": true
    }
  }
}
